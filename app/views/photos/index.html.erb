<div class="container">
  <div class="row">
    <div class="col s12">
      <h1>All photos for Project #<%= @current_project.id %></h1>

      <ul>
        <% @all_photos.each do |photo| %>
        <li>
          <h3><%= photo.title %></h3>
          <img src="<%= photo.url %>" alt="<%= photo.title %>">
        </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
